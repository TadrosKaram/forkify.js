  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.15.4",
    "gh-pages": "^3.2.3",
    "parcel": "^2.0.0-beta.1",
    "rimraf": "^5.0.10"
  },
  "homepage": "https://TadrosKaram.github.io/forkify.js"
}
